package com.mypractice;
class Parent{
	  //inheritance example

	 
	public void m1() {
		System.out.println("parent class");
		
	}}
	class Child extends Parent{
	public void m2() {
		System.out.println("Child class");
	}
	
}
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Parent p= new Parent();
		p.m1();
		p.m2();
	}

}
